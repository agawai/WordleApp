{
  "expo": {
    "name": "WordleApp",
    "slug": "WordleApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "wordle",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#121213"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
